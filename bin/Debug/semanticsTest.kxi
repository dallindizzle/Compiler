class Cat {
	
	Cat() {
	
	}

	private int Double(int x) {
		return x + x;
	}

	public void PrintDog(Dog d) {
		cout << d.age;
		cout << '\n';
		cout << Double(d.age);

	}
}

class Dog {
	public int age;

	Dog(int age) {
		this.age = age;
	}
}

void kxi2019 main() {
	Dog d = new Dog(5);
	Cat c = new Cat();

	c.PrintDog(d);

	cout << '\n';

	while(d.age <= 10) {
		cout << d.age;
		d.age = d.age + 1;
	}

}